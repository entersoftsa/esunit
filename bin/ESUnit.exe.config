<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.diagnostics>
		<switches>
			<add name="REMOTE" value="1" />
		</switches>
	</system.diagnostics>
	<appSettings>
		<!--   The number of records per request that the client can fetch from the application server.-->
		<add key="MaxNoRecords" value="0" />
		
		<!--   Indicates whether this EXE will run within the LAN or remote over low bandwith connection i.e. internet.-->
		<add key="LAN" value="true" />
		
		<add key="LiveSupportURL" value="http://mail.entersoft.gr/ESLiveSupport/LiveSupport.asmx" />
		<add key="ESLicense.es.ESActivationWEB" value="http://mail.entersoft.gr/ESActivationWEB/ESActivationWEB.asmx" />
	</appSettings>
	<runtime>
		<legacyUnhandledExceptionPolicy enabled="1" />
    <NetFx40_LegacySecurityPolicy enabled="true"/>
	</runtime>
	<startup useLegacyV2RuntimeActivationPolicy="true" />
</configuration>
